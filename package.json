{
  "name": "osm-countries-bounds",
  "version": "0.0.1",
  "description": "Package for node.js which contain countries with bounds and bbox in geojson format",
  "scripts": {
    "update-list": "node -r ts-node/register src/update-list.ts",
    "test": "mocha -r ts-node/register test/*.spec.ts",
    "server": "node -r ts-node/register src/server.ts",
    "server:watch": "nodemon -e js,ts,json --exec 'npm run server'",
    "build": "tsc",
    "lint": "eslint src/** --ext .ts,.tsx",
    "lint:fix": "eslint src/** --ext .ts,.tsx --fix"
  },
  "files": [
    "lib/**/*"
  ],
  "main": "lib/src/index.js",
  "typings": "lib/src/index.d.ts",
  "author": "Gabdrashitov Rinat <paladin2012gnu@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grinat/osm-countries-bounds.git"
  },
  "license": "MIT, Data Â© OpenStreetMap contributors, ODbL 1.0. https://osm.org/copyright",
  "dependencies": {
    "@turf/simplify": "^5.1.5",
    "@turf/union": "^6.0.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.0",
    "express": "^4.17.1",
    "iso3166-1": "^0.5.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5",
    "got": "^10.4.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.8",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/mocha": "^2.2.48",
    "@types/node": "^12.12.26",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.20.1",
    "mocha": "^7.0.1",
    "nodemon": "^2.0.2",
    "typescript-eslint-parser": "^22.0.0"
  }
}
